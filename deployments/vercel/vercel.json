{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "orchestrator/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "orchestrator/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "dashboard/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "TEXLIVE_URL": "@texlive_url"
  }
}
